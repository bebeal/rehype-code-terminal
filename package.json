{
  "name": "@bebeal/rehype-code-terminal",
  "description": "A rehype plugin for rendering code terminals in place of code blocks.",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/rehype-code-terminal.js",
  "module": "./dist/rehype-code-terminal.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/rehype-code-terminal.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/rehype-code-terminal.js"
      }
    },
    "./style.css": "./dist/assets/rehype-code-terminal.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --config demo/vite.config.ts",
    "build": "rimraf dist && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@bebeal/ripple-effect": "^1.0.0",
    "@mdx-js/react": "^3.1.0",
    "@mdx-js/rollup": "^3.1.0",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@wooorm/starry-night": "^3.7.0",
    "mdast-util-mdx-jsx": "^3.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rehype-starry-night": "^2.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/node": "^22.15.3",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "eslint-plugin-mdx": "^3.4.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "vite-plugin-dts": "^4.5.3"
  },
  "packageManager": "yarn@4.9.1"
}
